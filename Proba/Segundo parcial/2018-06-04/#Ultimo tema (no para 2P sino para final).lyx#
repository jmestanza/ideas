\begin_layout Standard

\series bold
Problema 11 - Guía 9:
\end_layout

\begin_layout Standard
11) X: tiempo de una operación
\end_layout

\begin_layout Standard
X~N(
\begin_inset Formula $\mu$
\end_inset

,
\begin_inset Formula $\sigma$
\end_inset

)
\end_layout

\begin_layout Standard
n=15
\end_layout

\begin_layout Standard
\begin_inset Formula $\overline{X}=\frac{1}{15}\sum_{i=1}^{15}X_{i}=\frac{76.9}{15}\approx5.13$
\end_inset


\end_layout

\begin_layout Standard
1-
\begin_inset Formula $\alpha$
\end_inset

=0.95
\end_layout

\begin_layout Standard
E = t
\begin_inset Formula $_{14,0.975}\frac{s}{\sqrt{15}}$
\end_inset


\end_layout

\begin_layout Standard
Muestra una recta con 
\end_layout

\begin_layout Standard
5.13 en el medio y 5.13-E y 5.13 + E
\end_layout

\begin_layout Standard
a veces da la suma de los xi y la suma de (xi^2) en los examenes
\end_layout

\begin_layout Standard
s
\begin_inset Formula $^{2}$
\end_inset

=
\begin_inset Formula $\frac{1}{14}\sum_{i=1}^{15}(X_{i}-5.13)^{2}=\frac{1}{14}(\sum_{i=1}^{15}X_{i}^{2}-2.513\sum_{i=1}^{15}X_{i}+15.513^{2})$
\end_inset

=
\begin_inset Formula $\frac{1}{14}(\sum_{i=1}^{15}X_{i}^{2}-15.513^{2})$
\end_inset

= (*)
\end_layout

\begin_layout Standard
Se da por conocida la suma
\end_layout

\begin_layout Standard
\begin_inset Formula $\sum_{i=1}^{15}X_{i}$
\end_inset

 =15.513
\end_layout

\begin_layout Standard
\begin_inset Formula $\sum_{i=1}^{15}X_{i}^{2}$
\end_inset

 =415.55
\end_layout

\begin_layout Standard
(*) =1.485 
\begin_inset Formula $\implies$
\end_inset

s=1.219
\end_layout

\begin_layout Standard
Entonces 
\end_layout

\begin_layout Standard
E = t
\begin_inset Formula $_{14,0.975}\frac{1.219}{\sqrt{15}}$
\end_inset

=2.1415
\begin_inset Formula $\frac{1.219}{\sqrt{15}}$
\end_inset

=0.675
\end_layout

\begin_layout Standard
14 grados de libertad la que deja 0.975 a la izquierda
\end_layout

\begin_layout Standard
Resultados muestrales:
\end_layout

\begin_layout Standard
\begin_inset Formula $\overline{X}=5.13$
\end_inset


\end_layout

\begin_layout Standard
S=1.22
\end_layout

\begin_layout Standard
sospecha que mu es mayor a 5 minutos
\end_layout

\begin_layout Standard
por ende vamos por la prueba de cola derecha
\end_layout

\begin_layout Standard
H
\begin_inset Formula $_{0}$
\end_inset

: 
\begin_inset Formula $\mu$
\end_inset


\begin_inset Formula $\leq$
\end_inset

5
\end_layout

\begin_layout Standard
\begin_inset Formula $H_{1}$
\end_inset

: 
\begin_inset Formula $\mu>5$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\alpha$
\end_inset

=0.05
\end_layout

\begin_layout Standard
\begin_inset Formula $t_{c}$
\end_inset

= t
\begin_inset Formula $_{14,0.95}\approx1.76$
\end_inset


\end_layout

\begin_layout Standard
t
\begin_inset Formula $_{obs}=\frac{5.13-5}{\frac{1.219}{\text{\sqrt{15}}}}=$
\end_inset


