\series bold
Estimación y prueba de hipótesis de la media poblacional 
\begin_inset Formula $\mu$
\end_inset


\series default
 (se usa estimador 
\begin_inset Formula $\overline{X}$
\end_inset

)
\end_layout

\begin_layout Standard
de una 
\bar under
población normal
\bar default
(X~N(
\begin_inset Formula $\mu,\sigma$
\end_inset

)) (Z=
\begin_inset Formula $\frac{\overline{X}-\mu}{\frac{\sigma}{\sqrt{n}}}\sim N(0,1)$
\end_inset

)
\end_layout

\begin_layout Standard
a partir de una muestra aleatoria (
\begin_inset Quotes eld
\end_inset

pequeñas muestras
\begin_inset Quotes erd
\end_inset

, n<30)
\end_layout

\begin_layout Standard
[para que sea independiente]
\end_layout

\begin_layout Standard
y 
\begin_inset Formula $\sigma$
\end_inset

 es conocido
\end_layout

\begin_layout Standard
entonces se estima con
\end_layout

\begin_layout Standard
\begin_inset Formula $s=\sqrt{\frac{1}{n-1}\sum}\frac{}{}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout

$$s=
\backslash
sqrt{
\backslash
frac{1}{n-1}
\backslash
sum_{i=1}^{n}(x_{i}-
\backslash
overline{x})^{2}}$$
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
t de student:
\end_layout

\begin_layout Standard
T= 
\begin_inset Formula $\frac{\overline{X}-\mu}{\frac{s}{\sqrt{n}}}\sim t_{n-1}$
\end_inset

 (t de student)
\end_layout

\begin_layout Standard
tiene n-1 grados de libertad
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout

$$
\backslash
sum_{i=1}^{n}(x_{i}-
\backslash
overline{x})^{2}$$
\end_layout

\end_inset


\end_layout

\begin_layout Standard
si conozco n-1 numeros de esta suma puedo conocer el que me falta
\end_layout

\begin_layout Standard
por eso n-1 grados de libertad
\end_layout

\begin_layout Standard
ya que
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout

$$
\backslash
sum_{i=1}^{n}(x_{i}-
\backslash
overline{x})=0$$
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\nu$
\end_inset

 de student es el n-1 de grados de libertad (no confundir con el tamaño
 de la muestra)
\end_layout

\begin_layout Standard
E(T)=0
\end_layout

\begin_layout Standard
V(T)=
\begin_inset Formula $\frac{\nu}{\nu-2}$
\end_inset

 con 
\begin_inset Formula $\nu>2$
\end_inset


\end_layout

\begin_layout Standard
V(T)
\begin_inset Formula $\begin{array}{cc}
\rightarrow\\
n\rightarrow\infty
\end{array}$
\end_inset

1
\end_layout

\begin_layout Standard
